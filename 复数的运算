>> z1=3+4i;
>> a1=real(z1)

a1 =

     3

>> a2=imag(z1)

a2 =

     4

>> b1=abs(z1)

b1 =

     5

>> b2=angle(z1)

b2 =

    0.9273

>> c1=conj(z1)

c1 =

   3.0000 - 4.0000i

>> z2=complex(1:3,2:4)

z2 =

   1.0000 + 2.0000i   2.0000 + 3.0000i   3.0000 + 4.0000i

>> real(z2)

ans =

     1     2     3

>> imag(z2)

ans =

     2     3     4
