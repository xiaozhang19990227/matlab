>> A=pascal(4)

A =

     1     1     1     1
     1     2     3     4
     1     3     6    10
     1     4    10    20

>> eig(A)

ans =

    0.0380
    0.4538
    2.2034
   26.3047

>> R=chol(A)

R =

     1     1     1     1
     0     1     2     3
     0     0     1     3
     0     0     0     1

>> R'*R

ans =

     1     1     1     1
     1     2     3     4
     1     3     6    10
     1     4    10    20
