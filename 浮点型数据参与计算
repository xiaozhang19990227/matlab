  single

  123.4000

>> b1=a+true

b1 =

  single

  124.4000

>> b2=a+'a'

b2 =

  single

  220.4000

>> b3=a+10.5

b3 =

  single

  133.9000

>> c=double(678.9)

c =

  678.9000

>> d1=c+false

d1 =

  678.9000

>> d2=c+'hello'

