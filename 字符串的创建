>> s=['Bob';'Tom']

s =

  2×3 char 数组

    'Bob'
    'Tom'

>> c=char('Robinson','Tom')

c =

  2×8 char 数组

    'Robinson'
    'Tom     '

>> celldata=cellstr(c)

celldata =

  2×1 cell 数组

    {'Robinson'}
    {'Tom'     }

>> celldata{1}

ans =

    'Robinson'
